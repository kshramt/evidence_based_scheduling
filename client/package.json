{
  "name": "evidence_based_scheduling",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "homepage": ".",
  "prettier": {},
  "dependencies": {
    "@bufbuild/connect": "^0.13.0",
    "@bufbuild/connect-web": "^0.13.0",
    "@bufbuild/protobuf": "^1.3.3",
    "@emotion/react": "^11.11.1",
    "@fontsource/material-icons": "^5.0.7",
    "@kshramt/fast-json-patch": "^3.1.2",
    "@kshramt/runtime-type-validator": "^0.10.1",
    "@kshramt/sequence-comparisons": "^0.4.0",
    "@mantine/core": "^6.0.20",
    "@mantine/dates": "^6.0.20",
    "@mantine/form": "^6.0.20",
    "@mantine/hooks": "^6.0.20",
    "@mantine/notifications": "^6.0.20",
    "@types/react-window": "^1.8.7",
    "dayjs": "^1.11.10",
    "idb": "^7.1.1",
    "immer": "^10.0.3",
    "jotai": "^2.4.3",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-virtuoso": "^4.6.2",
    "react-window": "^1.8.9",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "playwright test -c playwright-ct.config.ts"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/strict-type-checked",
      "plugin:import/recommended",
      "plugin:import/typescript",
      "plugin:jsx-a11y/recommended",
      "plugin:react-hooks/recommended",
      "plugin:react-hooks/recommended",
      "plugin:react/recommended",
      "plugin:testing-library/react"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "project": "./tsconfig.json",
      "tsconfigRootDir": "."
    },
    "rules": {
      "@typescript-eslint/no-confusing-void-expression": "off",
      "@typescript-eslint/no-dynamic-delete": "off",
      "@typescript-eslint/no-invalid-void-type": "off",
      "@typescript-eslint/no-misused-promises": [
        "error",
        {
          "checksVoidReturn": false
        }
      ],
      "@typescript-eslint/no-unnecessary-condition": [
        "error",
        {
          "allowConstantLoopConditions": true
        }
      ],
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "^_$",
          "argsIgnorePattern": "^_$",
          "destructuredArrayIgnorePattern": "^_$"
        }
      ],
      "@typescript-eslint/restrict-template-expressions": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/label-has-associated-control": [
        "error",
        {
          "controlComponents": [
            "GanttZoomSelector",
            "LocalTimeField"
          ]
        }
      ],
      "no-constant-condition": [
        "error",
        {
          "checkLoops": false
        }
      ],
      "react/display-name": "off",
      "react/prop-types": "off"
    },
    "settings": {
      "import/resolver": {
        "typescript": {
          "alwaysTryTypes": true,
          "project": "./tsconfig.json"
        }
      },
      "react": {
        "version": "detect"
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@bufbuild/buf": "^1.27.2",
    "@bufbuild/protoc-gen-connect-es": "^0.13.0",
    "@bufbuild/protoc-gen-es": "^1.3.3",
    "@playwright/experimental-ct-react": "^1.39.0",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-react-swc": "^3.4.1",
    "@vitest/browser": "^0.34.6",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.52.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.1.0",
    "playwright": "^1.39.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.5",
    "ts-prune": "^0.10.3",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-pwa": "^0.16.6",
    "vitest": "^0.34.6"
  }
}
