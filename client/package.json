{
  "name": "evidence_based_scheduling",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "homepage": ".",
  "prettier": {},
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@fontsource/material-icons": "^5.0.18",
    "@kshramt/fast-json-patch": "^3.1.2",
    "@kshramt/runtime-type-validator": "^0.12.0",
    "@kshramt/sequence-comparisons": "^0.5.0",
    "@mantine/core": "^7.9.2",
    "@mantine/dates": "^7.9.2",
    "@mantine/form": "^7.9.2",
    "@mantine/hooks": "^7.9.2",
    "@mantine/notifications": "^7.9.2",
    "@reduxjs/toolkit": "^2.2.5",
    "@types/react-window": "^1.8.8",
    "@zodios/core": "^10.9.6",
    "dayjs": "^1.11.11",
    "idb": "^8.0.0",
    "immer": "^10.1.1",
    "jotai": "^2.8.0",
    "react": "19.0.0-beta-26f2496093-20240514",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "19.0.0-beta-26f2496093-20240514",
    "react-redux": "^9.1.2",
    "react-virtuoso": "^4.7.10",
    "react-window": "^1.8.10",
    "workbox-core": "^7.1.0",
    "workbox-expiration": "^7.1.0",
    "workbox-precaching": "^7.1.0",
    "workbox-routing": "^7.1.0",
    "workbox-strategies": "^7.1.0",
    "zod": "^3.23.8"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "playwright test -c playwright-ct.config.ts"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/strict-type-checked",
      "plugin:import/recommended",
      "plugin:import/typescript",
      "plugin:jsx-a11y/recommended",
      "plugin:react-hooks/recommended",
      "plugin:react-hooks/recommended",
      "plugin:react/recommended",
      "plugin:testing-library/react"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "project": "./tsconfig.json",
      "tsconfigRootDir": "."
    },
    "plugins": [
      "eslint-plugin-react-compiler"
    ],
    "rules": {
      "@typescript-eslint/no-confusing-void-expression": "off",
      "@typescript-eslint/no-dynamic-delete": "off",
      "@typescript-eslint/no-invalid-void-type": "off",
      "@typescript-eslint/no-misused-promises": [
        "error",
        {
          "checksVoidReturn": false
        }
      ],
      "@typescript-eslint/no-unnecessary-condition": "off",
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "^_$",
          "argsIgnorePattern": "^_$",
          "destructuredArrayIgnorePattern": "^_$"
        }
      ],
      "@typescript-eslint/restrict-template-expressions": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/label-has-associated-control": [
        "error",
        {
          "controlComponents": [
            "GanttZoomSelector",
            "LocalTimeField"
          ]
        }
      ],
      "no-constant-condition": [
        "error",
        {
          "checkLoops": false
        }
      ],
      "react-compiler/react-compiler": "error",
      "react/display-name": "off",
      "react/jsx-uses-react": "off",
      "react/prop-types": "off",
      "react/react-in-jsx-scope": "off"
    },
    "settings": {
      "import/resolver": {
        "typescript": {
          "alwaysTryTypes": true,
          "project": "./tsconfig.json"
        }
      },
      "react": {
        "version": "detect"
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@playwright/experimental-ct-react": "^1.44.0",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/browser": "^1.6.0",
    "@vitest/coverage-istanbul": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "babel-plugin-react-compiler": "0.0.0-experimental-592953e-20240517",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-compiler": "0.0.0-experimental-c8b3f72-20240517",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-testing-library": "^6.2.2",
    "openapi-zod-client": "^1.18.1",
    "playwright": "^1.44.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "ts-prune": "^0.10.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-pwa": "^0.17.5",
    "vitest": "^1.6.0"
  }
}
